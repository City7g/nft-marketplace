mixin nft-creator(x)
  a(href="/artist.html").creator
    .creator__image
      p.text-p2.creator__number= x
      +image(`/img/avatars/item-${x}`).creator__img
    .creator__content
      h4.title-h5.creator__name Keepitreal
      p.text-p2.creator__info
        span.creator__text Total Sales:
        span.text-p2s.creator__price 34.53 ETH